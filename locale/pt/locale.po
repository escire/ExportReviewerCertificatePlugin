msgid "plugins.generic.exportReviewerCertificate.displayName"
msgstr "Exportar certificado de revisor"

msgid "plugins.generic.exportReviewerCertificate.description"
msgstr "Este plugin permite que revisores exportem seus certificados por itens revisados."

msgid "plugins.generic.exportReviewerCertificate.tabname"
msgstr "Certificado de revisor"




msgid "plugins.generic.exportReviewerCertificate.designDocumentSettings.label"
msgstr "Configurações de layout do certificado"

msgid "plugins.generic.exportReviewerCertificate.designDocumentSettings.description"
msgstr "Esta seção gerencia os detalhes do design do certificado."

msgid "plugins.generic.exportReviewerCertificate.designDocumentSettings.watermark.label"
msgstr "Marca d'água do certificado"

msgid "plugins.generic.exportReviewerCertificate.designDocumentSettings.watermark.description"
msgstr "É recomendado usar imagens PNG quadradas com 1000px de largura e 1000px de altura e fundo branco ou transparente."

msgid "plugins.generic.exportReviewerCertificate.designDocumentSettings.header.label"
msgstr "Cabeçalho do certificado"

msgid "plugins.generic.exportReviewerCertificate.designDocumentSettings.header.description"
msgstr "Recomenda-se usar imagens PNG com 600px de largura e 110px de altura e fundo branco ou transparente."

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.label"
msgstr "Configurações de conteúdo do certificado"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.description"
msgstr "Esta seção gerencia os detalhes do conteúdo do certificado."

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateGreeting.label"
msgstr "Saudação do certificado"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateGreeting.description"
msgstr "Insira a saudação do certificado (exemplo, a quem possa interessar:)"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateContent.label"
msgstr "Conteúdo do certificado"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateContent.description"
msgstr "Insira o conteúdo do certificado. Neste campo é necessário adicionar tags especiais, que estão listadas abaixo junto com sua descrição:<br><ul><li>{{reviewer_gender}}: Gênero do revisor</ li><li>{{reviewer_title }}: Título do revisor</li><li>{{reviewer_fullname}}: Nome completo do revisor</li><li>{{reviewer_institution}}: Instituição à qual o revisor o revisor pertence</li><li> {{publication_title}}: Título da publicação que você revisou </li></ul>Clicando no botão avobe <span class=\"fa fa-question-circle\">< /span> você verá um exemplo de como escrever esta seção."

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateInstitutionDescription.label"
msgstr "Informações do diário do certificado"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateInstitutionDescription.description"
msgstr "Insira as informações do diário que emite o certificado. Você pode incluir o que considerar necessário."

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateDate.label"
msgstr "Data de emissão do certificado"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateDate.description"
msgstr "Insira o conteúdo da data de emissão do certificado. Neste campo é necessário adicionar tags especiais, que estão listadas abaixo junto com sua descrição:<br> <ul> <li>{{day_number}}: Dia da expedição em formato numérico</ li> <li>{{day_text}}: Dia da expedição em formato de texto</ li> <li>{{month_name}}: Mês da expedição</li> <li>{{year_number}}: Ano da expedição</li> <li>{{today_day_number}}: Hoje dia da expedição em formato numérico</li> <li>{{today_day_text}}: Hoje dia da expedição em formato de texto</li> <li>{{today_month_name}}: Hoje, mês da expedição</li> <li>{{today_year_number}}: Hoje, ano da expedição</li> </ul>Clicando no botão <span class=\" fa fa-question-circle\"></span> acima você pode ver um exemplo de como escrever esta seção."

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateGoodbye.label"
msgstr "Certificado de adeus"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateGoodbye.description"
msgstr "Insira o conteúdo do certificado de adeus. (Exemplo, Atenciosamente)."

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.label"
msgstr "Configurações de assinatura de certificado"

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.description"
msgstr "Esta seção gerencia os detalhes de assinatura do certificado."

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.certificateEditorSignature.label"
msgstr "Assinatura do editor (fundo transparente)"

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.certificateEditorSignature.description"
msgstr "É recomendado usar imagem png quadrada es com fundo transparente com 500px de largura e 500px de altura."

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.certificateEditorName.label"
msgstr "Nome do editor"

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.certificateEditorInstitution.label"
msgstr "Instituição do editor"

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.certificateEditorEmail.label"
msgstr "E-mail do editor"





