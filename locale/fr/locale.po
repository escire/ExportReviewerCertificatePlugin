msgstr "Sélectionner une option"

msgid "plugins.generic.exportReviewerCertificate.reviewer.maleOption"
msgstr "Homme"

msgid "plugins.generic.exportReviewerCertificate.reviewer.femaleOption"
msgstr "Femme"

msgid "plugins.generic.exportReviewerCertificate.reviewer.independentTypeOption"
msgstr "Indépendant"

msgid "plugins.generic.exportReviewerCertificate.reviewer.institutionTypeOption"
msgstr "Institution"

msgid "plugins.generic.exportReviewerCertificate.reviewer.genre_label"
msgstr "Veuillez choisir le genre que vous souhaitez activer :"

msgid "plugins.generic.exportReviewerCertificate.reviewer.title_label"
msgstr "Veuillez saisir le titre que vous souhaitez afficher :"

msgid "plugins.generic.exportReviewerCertificate.reviewer.institution_type_label"
msgstr "Veuillez choisir une option de type d'institution :"

msgid "plugins.generic.exportReviewerCertificate.reviewer.institution_label"
msgstr "Veuillez saisir la préposition masculine ou féminine suivie du nom de votre établissement (Exemples : de l'Université, de l'Institut):"

msgid "plugins.generic.exportReviewerCertificate.reviewer.button_label"
msgstr "Générer un certificat"

msgid "plugins.generic.exportReviewerCertificate.reviewer.button_title"
msgstr "Télécharger le certificat de fin d'évaluation à partir de l'article"





