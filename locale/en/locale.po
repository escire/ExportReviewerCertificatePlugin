msgid "plugins.generic.exportReviewerCertificate.name"
msgstr "Export reviewer certificate"

msgid "plugins.generic.exportReviewerCertificate.description"
msgstr "This plugin allow reviewers export their certificates by reviewed items."

msgid "plugins.generic.exportReviewerCertificate.tabname"
msgstr "Reviewer certificate"




msgid "plugins.generic.exportReviewerCertificate.designDocumentSettings.label"
msgstr "Certificate layout settings"

msgid "plugins.generic.exportReviewerCertificate.designDocumentSettings.description"
msgstr "This section manages the details of the certificate design."

msgid "plugins.generic.exportReviewerCertificate.designDocumentSettings.watermark.label"
msgstr "Certificate watermark"

msgid "plugins.generic.exportReviewerCertificate.designDocumentSettings.watermark.description"
msgstr "It´s recommended to use square PNG images with 1000px width and 1000px height and white or transparent background."

msgid "plugins.generic.exportReviewerCertificate.designDocumentSettings.header.label"
msgstr "Certificate header"

msgid "plugins.generic.exportReviewerCertificate.designDocumentSettings.header.description"
msgstr "It´s recommended to use PNG images with 600px width and 110px height and white or transparent background."

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.label"
msgstr "Certificate content settings"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.description"
msgstr "This section manages the details of the certificate content."

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateGreeting.label"
msgstr "Certificate greeting"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateGreeting.description"
msgstr "Enter the certificate greeting (Example, To Whom It May Concern:)"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateContent.label"
msgstr "Certificate content"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateContent.description"
msgstr "Enter the content of the certificate. In this field it is necessary to add special tags, which are listed below along with their description:<br><ul><li>{{reviewer_gender}}: Gender of the reviewer</li><li>{{reviewer_title }}: Title of the reviewer</li><li>{{reviewer_fullname}}: Full name of the reviewer</li><li>{{reviewer_institution}}: Institution to which the reviewer belongs</li><li> {{publication_title}}: Title of the publication you reviewed </li></ul>By clicking the button avobe <span class=\"fa fa-question-circle\"></span> you will see an example of how to write this section."

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateInstitutionDescription.label"
msgstr "Certificate journal information"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateInstitutionDescription.description"
msgstr "Enter the journal information that issues the certificate. You can include what you consider necessary."

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateDate.label"
msgstr "Certificate issue date"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateDate.description"
msgstr "Enter the content of the certificate issuance date. In this field it is necessary to add special tags, which are listed below along with their description:<br> <ul> <li>{{day_number}}: Review expedition day on number format</li> <li>{{day_text}}: Review expedition day on text format</li> <li>{{ month_name}}: Review month of the expedition</li> <li>{{year_number}}: Review year of the expedition</li> <li>{{today_day_number}}: Today expedition day on number format</li> <li>{{today_day_text}}: Today expedition day on text format</li> <li>{{today_month_name}}: Today month of the expedition</li> <li>{{today_year_number}}: Today year of the expedition</li> </ul>By clicking the button <span class=\"fa fa-question-circle\"></span> above you can see an example of how to write this section."

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateGoodbye.label"
msgstr "Certificate goodbye"

msgid "plugins.generic.exportReviewerCertificate.contentDocumentSettings.certificateGoodbye.description"
msgstr "Enter the content of the certificate goodbye. (Example, Sincerely,)."

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.label"
msgstr "Certificate signing settings"

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.description"
msgstr "This section manages the certificate signing details."

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.certificateEditorSignature.label"
msgstr "Editor's signature (transparent background)"

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.certificateEditorSignature.description"
msgstr "It's recommended to use square png images with a transparent background with 500px width and 500px height."

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.certificateEditorName.label"
msgstr "Editor's name"

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.certificateEditorInstitution.label"
msgstr "Editor's institution"

msgid "plugins.generic.exportReviewerCertificate.certificateSignatureSettings.certificateEditorEmail.label"
msgstr "Editor's email"





